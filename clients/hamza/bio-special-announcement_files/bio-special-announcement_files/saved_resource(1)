
    var mysrc = "https://tracking.buygoods.com/track/?a=7125&firstcookie=0&referrer=&product=nervep1,nervep3,nervep6&sessid2=sessid20220202221629425&caller_url="+encodeURIComponent(window.location.href);
    if(typeof add_to_cart !== "undefined") 
    { 
        mysrc = mysrc+"&add_to_cart="+add_to_cart;  
    }
    var newScript = document.createElement("script");
    newScript.type = "text/javascript";
    newScript.async = true;
    newScript.src = mysrc;
    var s = document.getElementsByTagName("script")[0];
    s.parentNode.insertBefore(newScript, s);
    